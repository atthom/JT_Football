    <!-- http://polytech.unice.fr/cours/SI#Content_Distribution_in_Wireless_Networks -->

    <owl:NamedIndividual rdf:about="http://polytech.unice.fr/cours/SI#Content_Distribution_in_Wireless_Networks">
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#BigData"/>
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#Reseau"/>
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#SciencesDesDonnees"/>
        <ECTS rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">2.0</ECTS>
