    <!-- http://polytech.unice.fr/cours/SI#Analyse_et_indexation_dimages_et_de_videos_dans_de_grands_systmes_multimedia -->

    <owl:NamedIndividual rdf:about="http://polytech.unice.fr/cours/SI#Analyse_et_indexation_dimages_et_de_videos_dans_de_grands_systmes_multimedia">
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#AnalyseDeDonnees"/>
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#BDD"/>
