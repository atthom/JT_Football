    <!-- http://polytech.unice.fr/cours/SI#Environnements_Logiciels_pour_la_Programmation_Avance_de_Terminaux_Mobiles -->

    <owl:NamedIndividual rdf:about="http://polytech.unice.fr/cours/SI#Environnements_Logiciels_pour_la_Programmation_Avance_de_Terminaux_Mobiles">
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#Fonctionelle"/>
        <rdf:type rdf:resource="http://polytech.unice.fr/cours/SI#Informatique"/>
